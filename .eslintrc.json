{
  "extends": ["airbnb"],
  "parser": "babel-eslint",
  "rules": {
    "comma-dangle": 0,
    "no-console": 0,
    "semi": [2, "never"],
    "space-before-function-paren": 0,
    "no-multi-spaces": 0,
    "no-param-reassign": 0,
    "no-return-assign": 0,
    "no-confusing-arrow": [2, { "allowParens": true }],
    "no-use-before-define": [2, "nofunc"]
  },
  "plugins":["react"]
}
